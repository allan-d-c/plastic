{
  "uni": [
    {
      "univ-id": "${univ-id}",
      "name": "${uni-name}",
      "description": "${uni-description}",
      "service-type": "${uni-service-type}",
      "service-subtype": "${uni-service-subtype}",
      "admin-status": "${uni-admin-status}",
      "tenant-name": "${uni-tenant-name}",
      "vem-name": "${uni-vem-name}",
      "skip-deploy": "${uni-skip-deploy}",
      "endpoint": {
        "device": "${uni-endpoint-device}",
        "port": "${uni-endpoint-port}",
        "port-bandwidth": "${uni-endpoint-port-bandwidth}"
      },
      "ce-vlan-id-for-untagged-and-priority": "${uni-ce-vlan-id-for-untagged-and-priority}",
      "ce-vlan-ids": [
        {
          "vlan-id": "${uni-ce-vlan-id-entry}"
        }
      ],
      "uni-ce-data-profile": {
        "name": "${uni-ce-data-profile-name}",
        "description": "${uni-ce-data-profile-description}",
        "all-to-one-bundling-enabled": "${uni-ce-data-profile-all-to-one-bundling-enabled}",
        "bundling-enabled": "${uni-ce-data-profile-bundling-enabled}",
        "link-oam-enabled": "${uni-ce-data-profile-link-oam-enabled}",
        "speed": "${uni-ce-data-profile-speed}",
        "mode": "${uni-ce-data-profile-mode}",
        "mtu": "${uni-ce-data-profile-mtu}",
        "service-multiplexing-enabled": "${uni-ce-data-profile-service-multiplexing-enabled}",
        "activate": "${uni-ce-data-profile-activate}"
      }
    }
  ]
}
