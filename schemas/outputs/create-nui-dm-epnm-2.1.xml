<provision-service-request xmlns="urn:cisco:params:xml:ns:yang:service:activation:v1">
    <service-order-data>
        <customer-ref>${nui-tenant-name}</customer-ref>
        <service-type>carrier-ethernet-vpn</service-type>
        <service-subtype>UNI</service-subtype>
        <service-activate>${nui-admin-status}</service-activate>
        <termination-point-list>
            <termination-point-config>
                <tp-ref>${nui-endpoint}</tp-ref>
                <network-interface-name>${nui-id}</network-interface-name>
            </termination-point-config>
        </termination-point-list>
        <network-interface-list>
            <network-interface>
                <name>${nui-id}</name>
                <abc-data>
                    <activate>${nui-abc-data-profile-activate}</activate>
                    <description>${nui-abc-data-profile-description}</description>
                    <mtu>${nui-abc-data-profile-mtu}</mtu>
                    <speed>${nui-abc-data-profile-speed}</speed>
                    <mode>${nui-abc-data-profile-mode}</mode>
                    <service-multiplexing>${nui-abc-data-profile-service-multiplexing-enabled}</service-multiplexing>
                    <bundling>${nui-abc-data-profile-bundling-enabled}</bundling>
                    <enable-link-oam>${nui-abc-data-profile-link-oam-enabled}</enable-link-oam>
                    <enable-link-Management>${enable-link-management}</enable-link-Management>
                    <all-to-one-bundling>${nui-abc-data-profile-all-to-one-bundling-enabled}</all-to-one-bundling>
                </abc-data>
            </network-interface>
        </network-interface-list>
    </service-order-data>
</provision-service-request>
