<provision-service-request xmlns="service:activation:v1">
    <service-order-data>
        <customer-ref>${uni-tenant-name}</customer-ref>
        <!-- <service-type>{{ ${uni-service-type} == "CARRIER-ETHERNET" ? "carrier-ethernet-vpn" : "unknown" }}</service-type> -->
        <service-type>${uni-service-type}</service-type>
        <service-subtype>${uni-service-subtype}</service-subtype>
        <!-- <service-activate>{{ ${uni-admin-status} == "UP" ? true : false }}</service-activate> -->
        <service-activate>${uni-admin-status}</service-activate>
        <termination-point-list>
            <termination-point-config>
                <tp-ref>${uni-endpoint-port-bandwidth}</tp-ref>
                <network-interface-name>${univ-id}</network-interface-name>
            </termination-point-config>
        </termination-point-list>
        <network-interface-list>
            <network-interface>
                <name>${univ-id}</name>
                <ce-data>
                    <activate>${uni-ce-data-profile-activate}</activate>
                    <description>${uni-ce-data-profile-description}</description>
                    <mtu>${uni-ce-data-profile-mtu}</mtu>
                    <speed>${uni-ce-data-profile-speed}</speed>
                    <mode>${uni-ce-data-profile-mode}</mode>
                    <service-multiplexing>${uni-ce-data-profile-service-multiplexing-enabled}</service-multiplexing>
                    <bundling>${uni-ce-data-profile-bundling-enabled}</bundling>
                    <enable-link-oam>${uni-ce-data-profile-link-oam-enabled}</enable-link-oam>
                    <enable-link-Management>false</enable-link-Management>
                    <all-to-one-bundling>${uni-ce-data-profile-all-to-one-bundling-enabled}</all-to-one-bundling>
                </ce-data>
            </network-interface>
        </network-interface-list>
    </service-order-data>
</provision-service-request>
